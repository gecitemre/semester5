SELECT kingName, dateCrowned, Kingdom.kingdomName
FROM King, Kingdom
WHERE Kingdom.kingdomID=King.kingdomID
AND NOT EXISTS(SELECT * FROM War WHERE (War.kingdomID1=King.kingdomID OR War.kingdomID2=King.kingdomID) AND War.dateOfWar BETWEEN King.dateCrowned AND King.dateDied AND War.winnerID!=King.kingdomID)
AND EXISTS(SELECT * FROM War WHERE (War.kingdomID1=King.kingdomID OR War.kingdomID2=King.kingdomID) AND War.dateOfWar BETWEEN King.dateCrowned AND King.dateDied)
ORDER BY kingName ASC;